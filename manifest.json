{
	"manifest_version": 2,
	"name": "Simple Adblock",
	"description": "A simple Adblock for browsers",
	"version": "1.0.0",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	}
}
